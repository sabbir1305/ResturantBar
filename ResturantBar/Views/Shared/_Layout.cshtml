<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>Restaurant Bar</title>
    @*@Styles.Render("~/Content/css")*@
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
    <!-- animate.css -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css">
    <!-- featherlight -->
    <link href="~/Content/css/featherlight.min.css" rel="stylesheet" />
    <!-- google font -->
    <link href="https://fonts.googleapis.com/css?family=Indie+Flower" rel="stylesheet">
    <!-- css style -->
    <link href="~/Content/css/style.css" rel="stylesheet" />
</head>
<body ng-app="myapp">
    <section id="header" class="wow flipInX" data-wow-duration="3s" data-wow-delay="2s">
        <div class="wrapper">
            <nav class="navbar navbar-default" role="navigation">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#but1">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="#">Samdado</a>
                </div>
                <div class="collapse navbar-collapse" id="but1">
                    <ul class="nav navbar-nav">
                        <li><a href="#">Soup</a></li>
                        <li><a href="#">Ramen</a></li>
                        <li><a href="#">Mochi</a></li>
                        <li><a href="#">Tempura</a></li>
                    </ul>
                </div>
            </nav>
            <div class="content wow jello" data-wow-duration="10s">
                <!--<div class="content-box">-->
                <!--<img src="images/cake.jpg" class="img-responsive" alt="cake"> -->
                @Html.Action("Order", "Home")
                <!--</div>-->
            </div>
        </div>
    </section>

    <section id="photos">
        <div class="container">
            <div class="row">
                @RenderBody()
            </div>
        </div>
    </section>
    <!-- start form -->
    <section id="form">
        <div class="container">
            @Html.Action("SubscriberPartial", "Home")
        </div>
    </section>


    <!-- start footer -->
    <footer>
        <div class="container text-center wow zoomIn">
            <ul class="list-inline">
                <h2>Follow us</h2>
                <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                <li><a href="#"><i class="fa fa-instagram"></i></a></li>
                <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
            </ul>
            <h3 class="wow tada" data-wow-duration="2s">&copy ; by adams</h3>
        </div>
    </footer>



    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")*@

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="~/Content/js/jquery-3.2.1.min.js"></script>

    
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="~/Content/js/featherlight.min.js"></script>
    <script src="~/Content/js/wow.min.js"></script>
    <script src="~/Content/js/wow.js"></script>

    <script src="~/Content/js/bootstrap.min.js"></script>
    @Scripts.Render("~/bundles/angular")

        <script src="~/Angular/Module.js"></script>
        <script src="~/Angular/Subscriber.js"></script>
        <script src="~/Angular/Order.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
